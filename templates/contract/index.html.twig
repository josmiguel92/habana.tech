{% extends 'base.html.twig' %}

{% block title %}Contract index{% endblock %}

{% block body %}
    <h1>Contract index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Vendedor</th>
                <th>Representante_vendedor</th>
                <th>Objeto_contrato</th>
                <th>Precio_cfr</th>
                <th>Precio_sin_inspeccion</th>
                <th>Precio_inspeccion</th>
                <th>Precio_flete</th>
                <th>Moneda</th>
                <th>Tipo_carta_credito</th>
                <th>Puerto_origen</th>
                <th>Cantidad_embarques</th>
                <th>Cantidad_contenedores</th>
                <th>Tiempo_entrega</th>
                <th>Correo_comprador</th>
                <th>Tiempo_garantia</th>
                <th>Nombre_vendedor</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for contract in contracts %}
            <tr>
                <td>{{ contract.id }}</td>
                <td>{{ contract.vendedor }}</td>
                <td>{{ contract.representanteVendedor }}</td>
                <td>{{ contract.objetoContrato }}</td>
                <td>{{ contract.precioCfr }}</td>
                <td>{{ contract.precioSinInspeccion }}</td>
                <td>{{ contract.precioInspeccion }}</td>
                <td>{{ contract.precioFlete }}</td>
                <td>{{ contract.moneda }}</td>
                <td>{{ contract.tipoCartaCredito }}</td>
                <td>{{ contract.puertoOrigen }}</td>
                <td>{{ contract.cantidadEmbarques }}</td>
                <td>{{ contract.cantidadContenedores }}</td>
                <td>{{ contract.tiempoEntrega }}</td>
                <td>{{ contract.correoComprador }}</td>
                <td>{{ contract.tiempoGarantia }}</td>
                <td>{{ contract.nombreVendedor }}</td>
                <td>
                    <a href="{{ path('contract_show', {'id': contract.id}) }}">show</a>
                    <a href="{{ path('contract_edit', {'id': contract.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="18">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('contract_new') }}">Create new</a>
{% endblock %}
